{
  "name": "@slippi/slippi-js",
  "version": "6.1.3",
  "description": "Official Project Slippi Javascript SDK",
  "sideEffects": false,
  "license": "LGPL-3.0-or-later",
  "repository": "project-slippi/slippi-js",
  "author": {
    "name": "Jas Laferriere",
    "email": "jas.laferriere@gmail.com",
    "url": "https://github.com/JLaferri"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist"
  ],
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "browser": {
    "./src/utils/slpFile.ts": "./src/utils/browserUtils.ts",
    "./src/utils/slpStream.ts": "./src/utils/browserUtils.ts",
    "./src/utils/slpFileWriter.ts": "./src/utils/browserUtils.ts",
    "./src/console/index.ts": "./src/utils/browserUtils.ts",
    "./src/reading/nodeReading.ts": "./src/reading/browserReading.ts"
  },
  "module": "./dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn run clean",
    "start": "dts watch --target node",
    "build:check": "tsc --noEmit",
    "build": "tsc --noEmit && dts build --target node",
    "coverage": "dts test --runInBand --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "test": "dts test --runInBand",
    "test:watch": "dts test --runInBand --watchAll --coverage",
    "test:ci": "dts test --runInBand --ci --coverage",
    "lint": "dts lint src",
    "lint:fix": "dts lint src --fix",
    "watch": "webpack watch",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "prepublishOnly": "yarn run lint && yarn run test && yarn run build"
  },
  "keywords": [
    "slp-parser-js",
    "slp",
    "slippi",
    "melee",
    "ssbm"
  ],
  "dependencies": {
    "@shelacek/ubjson": "^1.0.1",
    "bufferutil": "^4.0.6",
    "encoding-japanese": "^1.0.30",
    "enet": "^0.2.9",
    "events": "^3.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.27.0",
    "reconnect-core": "^1.3.0",
    "semver": "^7.3.2",
    "utf-8-validate": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@types/bluebird": "^3.5.36",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.3",
    "@types/reconnect-core": "^1.3.1",
    "@types/semver": "^6.0.2",
    "buffer": "^6.0.3",
    "dts-cli": "^1.2.0",
    "husky": "^4.2.5",
    "open-cli": "^5.0.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.0",
    "ts-loader": "^9.2.6",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "util": "^0.12.4",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "yarn lint --quiet"
    }
  }
}
